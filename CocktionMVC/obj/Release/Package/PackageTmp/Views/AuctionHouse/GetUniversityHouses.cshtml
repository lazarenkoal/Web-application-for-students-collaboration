@{
    ViewBag.Title = "Дома в " + Model[0].University;
    int i = 0;
}
@model List<CocktionMVC.Models.DAL.House>
<h3>Дома в @Model[0].University</h3>
<div class="row">
    <div class="col-lg-4">
        @foreach (var house in Model)
        {
            <p style="font-weight: bold">@house.University</p>
            <p>Факультет: @house.Faculty</p>
            <p>Адрес: @house.Address</p>
            <p>@Html.ActionLink("Хочу посмотреть", "GetCurrentAuctionHouse", "AuctionHouse", new {id = house.Id}, null)</p>
        }
    </div>
    <div class="col-lg-8">
        <p>В этих домах много интересного происходит!!!</p>
    </div>
</div>
